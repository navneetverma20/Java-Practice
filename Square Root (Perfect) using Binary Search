import java.util.*;
public class Main
{
    static int BinarySearch(int n){
        int high = n;
		int low = 0;
		while(high>=low){
		    int mid = low + (high-low)/2;
		    if(mid*mid == n){
		        return mid;
		    }
		    else if(mid*mid > n){
		        high = mid-1;
		    }
		    else if(mid*mid < n){
		        low = mid+1;
		    }
		}
		return -1;
    }
	public static void main(String[] args) {
		System.out.print("Enter the number: ");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		System.out.println(BinarySearch(n));
	}
}
